{
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/v1/:path*",
      "destination": "/api/v1/index"
    },
    {
      "source": "/(directory|docs|status|explorer)(/.*)?",
      "destination": "/index.html"
    }
  ]
}
