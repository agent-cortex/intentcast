{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://discovery.agentcortex.space/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://discovery.agentcortex.space/health"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
